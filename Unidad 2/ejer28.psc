Algoritmo ejer28

	//Ejercicio 2.3 
	//Ejercicio 2.4
	//Ejercicio 2.8
	
	Escribir 'Ingrese valor A'
	Leer a
	Escribir 'Ingrese valor B'
	Leer b
	Escribir 'Ingrese valor C'
	Leer c
	
	x1 <- -b+(rc(b^2-4*a*c))/2*a
	x2 <- -b-(rc(b^2-4*a*c))/2*a
	
	Escribir 'x1 = ', x1
	Escribir 'x2 = ', x2
	

FinAlgoritmo


