Algoritmo ejer4_3
	
	Dimension apellido[6]
	
	//Lista Garcia
	
	Para c <- 1 Hasta 6 Hacer
		Escribir "Ingrese el apellido: "
		Leer apellido[c]
	FinPara

	Para c <- 1 Hasta 6 Hacer
		Si apellido[c] = "Garcia" Entonces
			Escribir "En la posición ",[c], " se encontró un Garcia"
		FinSi
	FinPara
	
	// Encontrar 1° Garcia
	
	
	
	
FinAlgoritmo